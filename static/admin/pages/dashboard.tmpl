<div class="row">

    <div class="col s12">
            <div class="marjen z-depth-1">

                <h5>Events</h5>

                <table class="striped">
                    <thead>
                        <tr>
                            <th>Event ID</th>
                            <th>Event State</th>
                            <th>Service Type</th>
                            <th>Service Port</th>
                            <th>Attacker Proto</th>
                            <th>Attacker IP</th>
                            <th>Attacker Port</th>
                            <th>When</th>
                            <th>Token/Sensor</th>
                        </tr>
                    </thead>
                    <tbody>
                    {{ if len .Events }}
                        {{ range $key, $v := .Events }}
                                <tr>
                                    <td>{{ $v.ID }}</td>
                                    <td>{{ $v.State }}</td>
                                    <td>{{ $v.SensorType }}</td>
                                    <td>{{ $v.SensorPort }}</td>
                                    <td>{{ $v.AttackerNetwork }}</td>
                                    <td>{{ $v.AttackerIP }}</td>
                                    <td>{{ $v.AttackerPort }}</td>
                                    <td>{{ prettytime $v.Created }}</td>
                                    <td><a class="btn-floating waves-effect waves-light teal lighten-1" href="/kushtaka/sensor/{{ $v.SensorID }}"><i class="material-icons">settings</i></a></td>
                                </tr>
                        {{ end }}
                    {{ end }}
                    </tbody>
                </table>
            </div>
    </div>

    <div class="col s12 center-align">
        {{ template "admin/partials/pagination" . }}
    </div>


</div>